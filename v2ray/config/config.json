		{
			"port": 80,
			"listen":"0.0.0.0",
			"protocol": "vmess",
			"settings": {
				"clients": [
					{
						"id": "$UUID",
						"alterId": 0
					}
				]
			},
			"streamSettings": {
				"network": "ws",
				"wsSettings": {
					"path": "/v2ray"
				}
			}
		},
		{
			"port": 443,
			"listen":"0.0.0.0",
			"protocol": "vmess",
