				"clients": [
					{
						"id": "$UUID",
						"alterId": 0
					}
				],
				"disableInsecureEncryption": true
			},
			"streamSettings": {
				"network": "ws",
				"wsSettings": {
					"path": "/v2ray"
				}
			}
		}
